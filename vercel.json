{
  "functions": {
    "client/dist/index.html": {
      "maxDuration": 10
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/client/dist/$1"
    }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": "client/dist",
  "installCommand": "npm install",
  "devCommand": "npm run dev"
}